{
	"trailingComma": "all",
	"useTabs": true,
	"tabWidth": 4,
	"singleQuote": true,
	"bracketSpacing": true,
	"astroAllowShorthand": false
  "importOrder": [
		"^@layouts/(.*)$",
		"^@components/(.*)$",
		"^@stores/(.*)$",
		"^@libs/(.*)$",
		"^@styles/(.*)$",
		"^@/(.*)$",
		"^[./]"
	],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
