{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"jsx": "preserve",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@layouts/*": ["./src/layouts/*"],
			"@libs/*": ["./src/libs/*"],
			"@stores/*": ["./src/stores/*"],
			"@styles/*": ["./src/styles/*"]
		}
	}
}
