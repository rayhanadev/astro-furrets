{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"types": ["unplugin-icons/types/react"],
		"target": "esnext",
		"module": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"jsx": "preserve",
		"baseUrl": ".",
		"paths": {
			"@layouts/*": ["./app/layouts/*"],
			"@components/*": ["./app/components/*"],
			"@stores/*": ["./app/stores/*"],
			"@libs/*": ["./app/libs/*"],
			"@styles/*": ["./app/styles/*"],
			"@/*": ["./app/*"]
		}
	}
}
