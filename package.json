{
	"name": "@example/basics",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"astro": "astro",
		"check": "astro check && tsc --noEmit",
		"build": "astro build",
		"clean": "npm run clean:dist && npm run clean:img",
		"clean:client": "rm -rf ./dist/client",
		"clean:dist": "rm -rf ./dist",
		"clean:img": "rm -rf ./node_modules/.cache/astro-imagetools",
		"clean:server": "rm -rf ./dist/server",
		"dev": "astro dev --host",
		"format": "prettier --check . --config ./prettier.config.json --ignore-path ./.gitignore",
		"format:fix": "prettier --write . --config ./prettier.config.json --ignore-path ./.gitignore",
		"lint": "eslint . --config ./eslint.config.json --ignore-path ./.gitignore",
		"lint:fix": "eslint . --config ./eslint.config.json --ignore-path ./.gitignore --fix",
		"start": "node index.mjs"
	},
	"browserslist": [
		"> 0.2% and not dead",
		"maintained node versions"
	],
	"dependencies": {
		"@astrojs/cloudflare": "^5.0.0",
		"@astrojs/node": "^4.0.0",
		"@astrojs/partytown": "^1.0.2",
		"@astrojs/react": "^1.2.2",
		"@astrojs/sitemap": "^1.0.0",
		"@astrojs/tailwind": "^2.1.3",
		"@iconify/json": "^2.1.155",
		"@koa/router": "^12.0.0",
		"@nanostores/react": "^0.4.1",
		"@replit/database": "^2.0.2",
		"astro": "^1.7.2",
		"astro-compress": "^1.1.24",
		"astro-google-fonts-optimizer": "^0.2.2",
		"astro-imagetools": "^0.6.17",
		"astro-og-canvas": "^0.1.5",
		"astro-robots-txt": "^0.3.10",
		"astro-seo": "^0.6.1",
		"astro-service-worker": "^0.1.2",
		"astro-webmanifest": "^0.4.2",
		"autoprefixer": "^10.4.13",
		"cssnano": "^5.1.14",
		"fastify": "^4.10.2",
		"koa-compress": "^5.1.0",
		"koa-helmet": "^6.1.0",
		"koa2-cors": "^2.0.6",
		"nanostores": "^0.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sharp": "^0.31.3",
		"tailwindcss": "^3.2.4",
		"unplugin-auto-import": "^0.12.1",
		"unplugin-icons": "^0.14.15",
		"koa": "^2.14.1",
		"koa-connect": "^2.1.0",
		"koa-static": "^5.0.0",
		"@ts-koa/koa-morgan": "^0.0.9"
	},
	"devDependencies": {
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@types/koa": "^2.13.5",
		"@types/koa-static": "^4.0.2",
		"@types/node": "^18.11.17",
		"@typescript-eslint/parser": "^5.47.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-astro": "^0.21.1",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"postcss-fontpath": "^1.0.0",
		"postcss-import": "^15.1.0",
		"prettier": "^2.8.1",
		"prettier-plugin-astro": "^0.7.0",
		"prettier-plugin-packagejson": "^2.3.0",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"sass": "^1.57.1"
	}
}
